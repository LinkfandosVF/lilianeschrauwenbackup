layout.advancedTextarea=function(){$('.nav-text-icons').each(function(){var $textFormat=$(this);var $textarea=$('#'+$textFormat.data('target'));$textarea.data('isPreview',!1);if(!$('#bbCodeModal').length){var $dom='<div class="modal hide fade" id="bbCodeModal">'+'<div class="modal-header">'+'<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>'+'<h3></h3>'+'</div>'+'<div class="modal-body">'+'</div>'+'<div class="modal-footer">'+'<input type="hidden" name="textarea_id" value="'+$textFormat.data('target')+'">'+'<a href="#" class="btn" data-dismiss="modal" aria-hidden="true">Annuler</a>'+'<a href="javascript:void(0)" class="btn btn-primary" onclick="layout.advancedTextarea.addTag()">Insérer</a>'+'</div>'+'</div>';$($dom).appendTo('body')}
var $bbCodeModal=$('#bbCodeModal');$textarea.on('mouseup keyup cut paste',function(){$(this).data({'start':$textarea.prop('selectionStart'),'end':$textarea.prop('selectionEnd'),'insText':$textarea.val().substring($textarea.prop('selectionStart'),$textarea.prop('selectionEnd'))})});$textFormat.find('.control-text-format-colors').each(function(){var $colors=$(this);$colors.on('click',function(e){e.stopPropagation()});$(this).find('span[style]').each(function(){$(this).on('click',function(){var $color=$(this).css('background-color');$colors.trigger('changeColor',[$color])})});$(this).find('button').on('click',function(){var $color=$colors.find('.control-text-format-colors-input').val();if(/(^#[0-9a-f]{6}$)|(^#[0-9a-f]{3}$)/gi.test($color)){$colors.find('.control-group').removeClass('error');$colors.trigger('changeColor',[$color])}else{$colors.find('.control-group').addClass('error')}});$(this).on('changeColor',function(e,color){layout.advancedTextarea.addTag('[color='+color+']','[/color]');$(this).closest('.open').removeClass('open')})});$textFormat.find('[data-modal]').each(function(){$(this).on({'click':function(e){if(!$textarea.data('isPreview')){var $tag=$(this).data('modal');switch($tag){case 'img':var $title='Insérer une image';var $options=[{'label':'Titre','name':'title','type':'text','value':$textarea.data('insText')},{'label':'Largeur','name':'width','type':'number'},{'label':'URL','name':'url','placeholder':'http://','required':!0,'type':'url'}];break;case 'quote':var $title='Citer';var $options=[{'label':'Auteur','name':'author','type':'text'},{'label':'Citation','name':'cite','required':!0,'type':'textarea','value':$textarea.data('insText')}];break;case 'size':var $title='Taille du texte';var $options=[{'label':'Taille en pixels','name':'size','required':!0,'type':'number'}];break;case 'spoiler':var $title='Texte caché';var $options=[{'label':'Intitulé','name':'label','type':'text','value':'spoiler'},{'label':'Texte','name':'content','required':!0,'type':'textarea','value':$textarea.data('insText')}];break;case 'url':var $title='Insérer un lien';var $options=[{'label':'Texte','name':'title','type':'text','value':$textarea.data('insText')},{'label':'URL','name':'url','placeholder':'http://','required':!0,'type':'url'}];break;case 'video':var $title='Insérer une vidéo';var $options=[{'label':'Hébergeur','name':'host','type':'select','options':[{'value':'youtube','label':'Youtube'},{'value':'dailymotion','label':'Dailymotion'}]},{'label':'URL','name':'id','placeholder':'Ex: https://youtu.be/YPaZAVEaTYM','required':!0,'type':'text','value':$textarea.data('insText')}]}
var $content='';for(var $i=0;$i<$options.length;$i ++){$content+='<div class="control-group'+($options[$i].required?' required':'')+'">'+'<label class="control-label" for="'+$options[$i].name+'">'+$options[$i].label+'</label>'+'<div class="controls">';if($options[$i].type=='textarea'){$content+='<textarea name="'+$options[$i].name+'" id="'+$options[$i].name+'" class="input-block-level">'+($options[$i].value?$options[$i].value:'')+'</textarea>'}else if($options[$i].type=='select'){$content+='<select name="'+$options[$i].name+'" id="'+$options[$i].name+'" class="input-block-level">';for(var $o=0;$o<$options[$i].options.length;$o ++){$content+='<option value="'+$options[$i].options[$o].value+'">'+$options[$i].options[$o].label+'</option>'}
$content+='</select>'}else{$content+='<input type="'+$options[$i].type+'" name="'+$options[$i].name+'" id="'+$options[$i].name+'" class="input-block-level" value="'+($options[$i].value?$options[$i].value:'')+'"'+($options[$i].placeholder?' placeholder="'+$options[$i].placeholder+'"':'')+'>'}
$content+='<input type="hidden" name="tag" value="'+$tag+'">'
'</div>'+'</div>'}
$bbCodeModal.find('h3').html($title);$bbCodeModal.find('.modal-body').html($content);$bbCodeModal.modal()}else{e.preventDefault()}}})});$textFormat.find('[data-tag]').each(function(){$(this).on({'click':function(e){if(!$textarea.data('isPreview')){layout.advancedTextarea.addTag($(this).data('tag'))}else{e.preventDefault()}}})});$textFormat.find('a[data-toggle="smileys"]').on('click',function(e){if(!$textarea.data('isPreview')){$(this).toggleClass('active');$(this).parents('form').find('.smiley-container').toggleClass('hide')}else{e.preventDefault()}});var $preview=$('<div class="bbcode-preview hide">').css({'border':$textarea.css('border')}).insertBefore($textarea);$textFormat.find('a[data-toggle="preview"]').on('click',function(){if(!$preview.hasClass('open')){$.ajax({url:'/themes/do/preview/',type:'post',data:{'message':$textarea.val()},dataType:'json'}).done(function(json){$preview.css({'top':$textarea.position().top+'px','left':$textarea.position().left+'px','width':$textarea.outerWidth()+'px','height':$textarea.outerHeight()+'px','padding':$textarea.css('padding'),'border-radius':$textarea.css('border-radius')}).html(json.content);$preview.find('.spoiler-btn').addClass('btn-mini').removeClass('btn-link').on('click',function(){layout.advancedTextarea.toggleSpoiler($(this))})});$(this).addClass('active btn-primary').find('i').removeClass('icon-ok').addClass('icon-remove')}else{$preview.html('');$textarea.focus();$(this).removeClass('active btn-primary').find('i').removeClass('icon-remove').addClass('icon-ok')}
$preview.toggleClass('open');$textFormat.find('.btn').not(this).toggleClass('disabled');$textarea.data('isPreview',$preview.hasClass('open'))})});$('.spoiler-btn').addClass('btn-mini').removeClass('btn-link').on('click',function(){layout.advancedTextarea.toggleSpoiler($(this))})}
layout.advancedTextarea.addTag=function($bbOpen,$bbClose){var $textarea=$('#'+$('#bbCodeModal').find('[name="textarea_id"]').val());if($('#bbCodeModal').find('.required').length){$('#bbCodeModal').find('.required').each(function(){var $requiredField=$('#'+$(this).find('label').attr('for'));if(!$requiredField.val()){$requiredField.closest('.required:not(.error)').addClass('error')}else{$requiredField.closest('.required.error').removeClass('error')}})}
if(!$('#bbCodeModal').find('.required.error').length){if($bbOpen==undefined){var $tag=$('#bbCodeModal').find('[name="tag"]').val();switch($tag){case 'img':$bbOpen='[img='+$('#bbCodeModal').find('#url').val()+' alt='+($('#bbCodeModal').find('#title').val()?$('#bbCodeModal').find('#title').val():$('#bbCodeModal').find('#url').val())+($('#bbCodeModal').find('#title').val()?' title='+$('#bbCodeModal').find('#title').val():'')+($('#bbCodeModal').find('#width').val()?' width='+$('#bbCodeModal').find('#width').val():'')+']';$bbClose='';$textarea.data('insText','');break;case 'quote':$bbOpen='[quote'+($('#bbCodeModal').find('#author').val()?'='+$('#bbCodeModal').find('#author').val():'')+']';$bbClose='[/quote]';$textarea.data('insText',$('#bbCodeModal').find('#cite').val());break;case 'size':$bbOpen='[size='+$('#bbCodeModal').find('#size').val()+']';$bbClose='[/size]';break;case 'spoiler':$bbOpen='[spoiler='+($('#bbCodeModal').find('#label').val()?$('#bbCodeModal').find('#label').val():'spoiler')+']';$bbClose='[/spoiler]';$textarea.data('insText',$('#bbCodeModal').find('#content').val());break;case 'url':$bbOpen='[url='+$('#bbCodeModal').find('#url').val()+']';$bbClose='[/url]';$textarea.data('insText',$('#bbCodeModal').find('#title').val()?$('#bbCodeModal').find('#title').val():$('#bbCodeModal').find('#url').val());break;case 'video':$bbOpen='['+$('#bbCodeModal').find('#host').val()+']';$bbClose='[/'+$('#bbCodeModal').find('#host').val()+']';$textarea.data('insText',$('#bbCodeModal').find('#id').val())}}
$bbClose=$bbClose===undefined?$bbOpen.replace('[','[\/'):$bbClose;var $start=$textarea.data('start');var $end=$textarea.data('end');var $insText=$textarea.data('insText');if($insText){var $pos=$start+$bbOpen.length+$insText.length+$bbClose.length}else{var $pos=$start+$bbOpen.length}
$textarea.trigger('focus');$textarea.val($textarea.val().substr(0,$start)+$bbOpen+$insText+$bbClose+$textarea.val().substr($end));$textarea.prop('selectionStart',$pos);$textarea.prop('selectionEnd',$pos);$textarea.trigger('keyup');$('#bbCodeModal').modal('hide')}else{return!1}}
layout.advancedTextarea.toggleSpoiler=function($spoilerBtn){$spoilerBtn.next('.spoiler-content').toggleClass('hide')}
layout.resizeSlider=function(sliderId,sliderHeight){var slider=$('#'+sliderId);var slider_width=slider.width();var slider_height=sliderHeight;if(slider_width<=480){slider_height=slider_height/2;slider.addClass('widget-slider-small')}else{slider.removeClass('widget-slider-small')}
slider.find('.carousel-inner').css('height',slider_height+'px');slider.find('.item img').each(function(){if($(this).get(0).height>slider_height){var img_ratio=$(this).get(0).height/slider_height;var img_width=$(this).get(0).width/img_ratio}else{var img_ratio=slider_height/$(this).get(0).height;var img_width=$(this).get(0).width*img_ratio}
if(img_width>slider_width){var margin_left=(img_width-slider_width)/2;$(this).css('margin-left','-'+margin_left+'px')}else{$(this).css('margin-left','')}})}
layout.formHorWidth=function(){$('.view form[class*="horizontal"]').each(function(){if($(this).innerWidth()<530){$(this).addClass('form-not-horizontal').removeClass('form-horizontal')}else{$(this).removeClass('form-not-horizontal').addClass('form-horizontal')}})}
layout.picturesMargins=function(){$('.pictures li').each(function(){if($(this).parent().find('li').length>1){var margin_left=$(this).parent().find('li:nth-child(2)').css('margin-left');$(this).css('margin-bottom',margin_left)}})}
var blocks={};blocks.change_page=function(page,blocks_rows){var current_page=Number(blocks_rows.attr('data-current-page'));var total_page=Number(blocks_rows.attr('data-total-pages'));var pagination_ul=blocks_rows.nextAll('.pagination:first');blocks_rows.find('.row-container').addClass('hide');pagination_ul.find('.disabled').removeClass('disabled');if(page=='first')
current_page=1;else if(page=='prev')
current_page=current_page==1?current_page:current_page-1;else if(page=='next')
current_page=current_page==total_page?current_page:current_page+1;else if(page=='last')
current_page=total_page;else current_page=page;if(current_page==1)
pagination_ul.find('.pagination-prev, .pagination-first').addClass('disabled');if(current_page==total_page)
pagination_ul.find('.pagination-next, .pagination-last').addClass('disabled');blocks_rows.find('.page_'+current_page).removeClass('hide');blocks_rows.find('.page_'+current_page+' .row-container').removeClass('hide');blocks_rows.find('.page_'+current_page+' .cls-mobile-flash').css('width','100%');blocks_rows.find('.page_'+current_page+' .cls-mobile-flash object').css('width','100%');pagination_ul.find('.pagination-current').removeClass('pagination-current active');pagination_ul.find('[data-page="'+current_page+'"]').addClass('pagination-current active');pagination_ul.find('.pagination-prev a').attr('href','#page'+(current_page));pagination_ul.find('.pagination-next a').attr('href','#page'+(current_page));$('html, body').animate({scrollTop:blocks_rows.parents('.view:first').offset().top},'slow');blocks_rows.attr('data-current-page',current_page);location.hash='page'+current_page;return!1};$(document).ready(function(){var blocks_rows=$('.rows[data-total-pages]');var $this,pagination_ul;blocks_rows.each(function(){var $this=$(this);if($(this).attr('data-total-pages')>1){pagination_ul=$this.nextAll('.pagination:first');pagination_ul.find('.pagination-first a').click($.proxy(blocks.change_page,blocks,'first',$this));pagination_ul.find('.pagination-prev a').click($.proxy(blocks.change_page,blocks,'prev',$this));pagination_ul.find('.pagination-next a').click($.proxy(blocks.change_page,blocks,'next',$this));pagination_ul.find('.pagination-last a').click($.proxy(blocks.change_page,blocks,'last',$this));var hash=window.location.hash;var current_page=hash.substring(1,5)=='page'?parseInt(hash.substring(5,hash.length),10):1;blocks.change_page(current_page,$this)}
$(this).find('.row-panel').each(function(){var $row=$(this);var $collapse=$row.find('.collapse');$row.addClass(function(){return $collapse.hasClass('in')?'open':''});$collapse.on({'show':function(){$this.find('.row-panel.open').find('.row-content').find('> .collapse.in').collapse('hide')},'shown':function(){$row.addClass('open')},'hidden':function(){$row.removeClass('open')}})})})});var paymeans={};paymeans.pbx={};paymeans.pbx.cmd1=null;paymeans.pbx.cmd3=null;paymeans.pbx.total1=null;paymeans.pbx.total3=null;paymeans.pbx.hmac1=null;paymeans.pbx.hmac3=null;paymeans.pbx.datevalmax1=null;paymeans.pbx.datevalmax3=null;paymeans.pbx.selectCard=function(type){$('#PBX_TYPECARTE').val(type);if($('input[name=pbx_3fois]:checked').val()=='3fois'){$('#PBX_CMD').val(this.cmd3);$('#PBX_TOTAL').val(this.total3);$('#PBX_HMAC').val(this.hmac3);$('#PBX_DATEVALMAX').val(this.datevalmax3)}else{$('#PBX_CMD').val(this.cmd1);$('#PBX_TOTAL').val(this.total1);$('#PBX_HMAC').val(this.hmac1);$('#PBX_DATEVALMAX').val(this.datevalmax1)}
