function isMobile(){try{document.createEvent("TouchEvent"); return true;} catch(e){return false;}}
var doc = {};
doc.init = function()
{
  var modal = '<div id="modal" class="modal hide fade" tabindex="-1" role="dialog" aria-hidden="true"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button><h3 id="myModalLabel">Modal header</h3></div><div class="modal-body"></div><div class="modal-footer"><button class="btn" data-dismiss="modal" aria-hidden="true">Close</button><button class="btn btn-primary">Save changes</button></div></div>';
  $('body').append(modal);
}
doc.alert = function(message)
{
  $('#modal .modal-body').html('<p class="lead">' + message + '</p>');
  if($('#modal').find('.fb_buttons').length > 0)
  {
 var b = $('#modal .modal-body').find('.fb_buttons');
 $('#modal .modal-footer').html(b.html());
 b.remove();
 $('#modal .modal-footer .button').addClass('btn').removeClass('button');
 $('#modal .modal-footer .btn[onclick]').attr({
   'data-dismiss' :  'modal',
   'aria-hidden'  :  'true'
 }).removeAttr('onclick').addClass('btn-primary');
  }
  $('#modal').modal();
}
doc.confirm = function(message, buttonConfirm, onConfirm, buttonCancel, onCancel)
{
  $('#modal .modal-body').html('<p class="lead">' + message + '</p>');
  $('#modal .modal-footer button[data-dismiss]').text(buttonCancel).on('click', onCancel);
  $('#modal .btn-primary').text(buttonConfirm).on('click', onConfirm);
  $('#modal').modal();
}
var modules = {};
  $(document).ready(function(){
  if(typeof(total_page) != 'undefined')
  {
 var hash = window.location.hash;
 if(hash.substring(1, 5)=='page')
 {
   current_page = parseInt(hash.substring(5, hash.length));
   changepage(current_page);
 }
 $('#pagination-pages .pagination-last a').attr('href','#page'+total_page);
 updatePagelinks();
  }
})
function changepage(page){
  $('#rows .row-container').addClass('hide');
  $('#pagination-pages .disabled').removeClass('disabled');
  if(page == 'first')
  {
   current_page = 1;
  }
  else if(page == 'prev')
  {
   current_page = current_page == 1 ? current_page : current_page - 1;
  }
  else if(page == 'next')
  {
   current_page = current_page == total_page ? current_page : current_page + 1;
  }
  else if(page == 'last')
  {
   current_page = total_page; 
  }
  if(current_page == 1)
  {
 $('#pagination-pages .pagination-prev, #pagination-pages .pagination-first').addClass('disabled');
  }
  if(current_page == total_page)
  {
 $('#pagination-pages .pagination-next, #pagination-pages .pagination-last').addClass('disabled');
  }
  $('#rows .page_' + current_page).removeClass('hide');
  $('#rows .page_'+current_page+' .cls-mobile-flash').css('width','100%');
  $('#rows .page_'+current_page+' .cls-mobile-flash object').css('width','100%');
  $('#pagination-pages span').text(current_page);
  updatePagelinks(current_page);
  $('html, body').animate({
 scrollTop: $('.view-pages').offset().top
  }, 'slow');
  return false;
}
function updatePagelinks(){
  $('#pagination-pages .pagination-prev').attr('href','#page'+(current_page));
  $('#pagination-pages .pagination-next').attr('href','#page'+(current_page));
}var initModules = function()
{
  for(var i in modules)
  {
   if(modules[i].init)
   {
  modules[i].init();
   }
  }
}
modules.member = {};
modules.member.orderUpdate = function()
{
  $('#order_form_task').val('update');
  $('#order_form').submit();
};
modules.member.orderSubmit = function()
{
  $('#order_form_task').val('create');
  $('#order_form').attr('action', '//www.lilianeschrauwen.be/members/do/subscription/');
  $('#order_form').submit();
};
modules.member.onPaymeanChange = function()
{
  modules.member.orderUpdate();
};
var plugins = {};
  plugins.comment = {};
plugins.comment.init = function()
{
}
plugins.comment.showMoreComments = function(nb_comments)
{
  $('.plugin-comments .hide:lt(' + nb_comments + ')').removeClass('hide');
  var numpage = $('.plugin-comments .hide').length;
  if(numpage==0)
  {
   $('#btn-more-comments').hide();
  }
}
  plugins.rating = {};
plugins.rating.init = function()
{
  $('.plugin-rating .rating-list li').mouseover(function() {
   $(this).nextAll('li').removeClass('note-checked').addClass('note-off');
   $(this).prevAll('li').removeClass('note-off').addClass('note-checked');
   $(this).removeClass('note-off').addClass('note-checked');
  });
  $('.plugin-rating .rating-list').mouseout(function() {
   $(this).children('li').removeClass('note-checked').addClass('note-off');
   $(this).children('li.note-always').removeClass('note-off').addClass('note-checked');
  });
  $('.plugin-rating .rating-list input').click(function(){
   $(this).parents('ul.rating-list').find('li').removeClass('note-checked');
   $(this).parent('li').addClass('note-checked');
   $(this).parents('form').submit();
  });
}  
plugins.smileys = {};
plugins.smileys.init = function()
{
  $('a.smiley').click(function(e){
 e.preventDefault();
 var smiley = $(this).attr('rel');
 var target = $(this).data('target');
 if (target && target != '')
 {
 var msg = $(this).parents('form').find('#' + target);
 }
 else
 {
 var msg = $(this).parents('form').find('textarea');
 }
 msg.val(msg.val() + smiley).focus();
  });
}
var initPlugins = function()
{
  for(var i in plugins)
  {
   if(plugins[i].init)
   {
  plugins[i].init();
   }
  }
}
var layout = new Object();
layout.init = function()
{  
 $('form').submit(function(){
 var validation = true;
 if($(this).find('.required').length > 0){
  var nb_errors = 0;
   var error_msg = 'Merci de complter les champs suivants :\n';
  $(this).find('.required input[type="text"]:not([data-exclude-validation]), .required input[type="password"], .required input[type="file"], .required textarea, .required select').each(function(){
  if($(this).val() == '')
  {
   nb_errors ++;
 error_msg += '- ' + ($(this).is('[title]') ? $(this).attr('title') : $(this).parents('.required').find('.control-label:visible').text()) + '\n';
 $(this).parents('.required:not(.error)').addClass('error');
  }
  else
  {
 $(this).parents('.required.error').removeClass('error');
  }
  });
   $(this).find('.required input[type="radio"], .required input[type="checkbox"]').each(function(){
  var element_name = $(this).attr('name');
  var index = $('input[name="' + element_name + '"]').index($(this));
  if(index == 0)
  {
   var nb_elements = $('input[name="' + element_name + '"]').length;
   var nb_checked_elements = 0;
   for(c = 0; c < nb_elements; c ++)
   {
   if($('input[name="' + element_name + '"]').eq(c).attr('checked') == 'checked')
   {
 nb_checked_elements ++;
   }
   }
   if(nb_checked_elements == 0)
   {
  nb_errors ++;
   var label = $(this).parents('.required').find('label').eq(0).text();
  error_msg += '- ' + $.trim(label) + '\n';
   $(this).parents('.required:not(.error)').addClass('error');
   }
 else
 {
   $(this).parents('.required.error').removeClass('error');
 }
  }
   });
  if(nb_errors > 0){
  alert(error_msg);
  validation = false;
  }
 }
 if($(this).hasClass('form-search')){
   var l = $(this).find('[name="q"]').val().length > 2;
   if(!l)
   {
  alert('Seuls les mots de plus de deux caractres sont pris en compte lors de la recherche.');
  validation = false;
   }
 }
 if(validation == true){
   $(this).find('[type="submit"]').addClass('disabled');
 }
 return validation;
 });
  if(window.matchMedia('(max-width: 979px)').matches)
  {
 $('.dropdown-toggle, .dropdown-submenu > a').each(function(){
   $(this).on('click', function(e){
  e.preventDefault();
  $(this).closest('li').toggleClass('open');
  $(this).closest('li').siblings('.dropdown.open').removeClass('open');
   });
 });
  }
  $('iframe[src*="youtube.com"], iframe[src*="dailymotion.com"], iframe[src*="player.vimeo.com"], object:has(param[name="movie"])').each(function(){
 if(!$(this).hasClass('fixSize') && !$(this).parent().hasClass('video-container'))
 {
   $(this).wrap('<span class="video-container"></span>');
 }
  });
  $('[data-tip]').toggle(function(){
 $(this).addClass('active');
  }, function() {
 $(this).removeClass('active');
  });
  $('[data-role="ordering"]').find('select').on('change', function(){
 var $el   = this;
 var $url  = $el.options[$el.selectedIndex].value;
 window.location.href = $url != '' ? '?orderby=' + $url : window.location.origin + window.location.pathname;
  });
  $('.carousel-thumbnails').each(function(){
 var carouselId  = $(this).attr('id').substring(0, $(this).attr('id').lastIndexOf('-thumbnails'));
 var carouselThumbnails = $(this);
 $('#' + carouselId).on('slid', function(){
   var currentImg = $(this).find('.active').find('img').attr('src');
   carouselThumbnails.find('a.active').removeClass('active');
   carouselThumbnails.find('img[src^="' + currentImg + '"]').parent().addClass('active');
 });
 carouselThumbnails.carousel({
   interval: false
 })
 .find('.carousel-inner')
 .find('a')
 .each(function(){
   $(this).on('click', function(e){
  e.preventDefault();
  var imgUrl = $(this).attr('href');
  var number = $('#' + carouselId).find('.item').index($('#' + carouselId).find('img[src="' + imgUrl + '"]').closest('.item'));
  $('#' + carouselId).carousel(number);
   });
 });
  });
  $("a.mb").attr("rel","");
  $("a.mb[href^='http://www.youtube.com/v/'], a.mb[href^='http://www.dailymotion.com/']").attr("rel","lightboxswf");
  $("a.mb[rel='lightboxswf']").removeClass("mb");
  $("a.mb img").parent().attr("rel","iframe");
  $("a.mb[href$='.jpg'],a.mb[href$='.gif'],a.mb[href$='.png'],a.mb[href$='.jpeg']").fancybox();
  $("a.lightbox, a.mb2, a.mb3, a[rel='iframe']").fancybox({'showNavArrows':true});
  $("a[rel='iframe'] img").parent().fancybox({'type':'image', 'showNavArrows':true});
  $("a[rel='lightboxswf']").fancybox({'type':'swf'});
  $("a[rel='fancybox']").fancybox();
  $(".iframe").fancybox({"type":"iframe"});
  $('[data-widget="image"][class*="align-"]').each(function(){
 var $className = $(this).hasClass('align-center') ? 'align-center' : 'align-bottom';
 $(this).removeClass($className).closest('.column').addClass($className);
  });
  layout.advancedTextarea();
}
layout.advancedTextarea = function()
{
  $('.nav-text-icons').each(function(){
 var $textFormat = $(this);
 var $textarea = $('#' + $textFormat.data('target'));
 $textarea.data('isPreview', false);
 if(!$('#bbCodeModal').length)
 {
   var $dom = 
   '<div class="modal hide fade" id="bbCodeModal">' + 
  '<div class="modal-header">' + 
 '<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>' + 
 '<h3></h3>' + 
  '</div>' + 
  '<div class="modal-body">' + 
  '</div>' + 
  '<div class="modal-footer">' + 
 '<input type="hidden" name="textarea_id" value="' + $textFormat.data('target') + '">' + 
 '<a href="#" class="btn" data-dismiss="modal" aria-hidden="true">Annuler</a>' + 
 '<a href="javascript:void(0)" class="btn btn-primary" onclick="layout.advancedTextarea.addTag()">Insrer</a>' + 
  '</div>' + 
   '</div>';
   $($dom).appendTo('body');
 }
 var $bbCodeModal = $('#bbCodeModal');
 $textarea.on('mouseup keyup cut paste', function(){
   $(this).data({
  'start' : $textarea.prop('selectionStart'),
  'end' : $textarea.prop('selectionEnd'),
  'insText' : $textarea.val().substring($textarea.prop('selectionStart'), $textarea.prop('selectionEnd'))
   });
 });
 $textFormat.find('.control-text-format-colors').each(function(){
   var $colors = $(this);
   $colors.on('click', function(e){
  e.stopPropagation();
   });
   $(this).find('span[style]').each(function(){
  $(this).on('click', function(){
 var $color = $(this).css('background-color');
 $colors.trigger('changeColor', [$color]);
  });
   });
   $(this).find('button').on('click', function(){
  var $color = $colors.find('.control-text-format-colors-input').val();
  if(/(^#[0-9a-f]{6}$)|(^#[0-9a-f]{3}$)/gi.test($color))
  {
 $colors.find('.control-group').removeClass('error');
 $colors.trigger('changeColor', [$color]);
  }
  else
  {
 $colors.find('.control-group').addClass('error');
  }
   });
   $(this).on('changeColor', function(e, color){
  layout.advancedTextarea.addTag('[color=' + color + ']', '[/color]');
  $(this).closest('.open').removeClass('open');
   });
 });
 $textFormat.find('[data-modal]').each(function(){
  $(this)
  .on({
  'click' : function(e){
 if(!$textarea.data('isPreview'))
 {
   var $tag = $(this).data('modal');
   switch ($tag){
  case 'img':
 var $title = 'Insrer une image';
 var $options = [
   {
  'label'  : 'Titre',
  'name'  : 'title',
  'type'  : 'text',
  'value'  : $textarea.data('insText')
   },
   {
  'label'  : 'Largeur',
  'name'  : 'width',
  'type'  : 'number'
   },
   {
  'label'  : 'URL',
  'name'  : 'url',
  'placeholder'  : 'http://',
  'required'   : true,
  'type'  : 'url'
   }
 ];
 break;
  case 'quote':
 var $title = 'Citer';
 var $options = [
   {
  'label'  : 'Auteur',
  'name'  : 'author',
  'type'  : 'text'
   },
   {
  'label'  : 'Citation',
  'name'  : 'cite',
  'required'   : true,
  'type'  : 'textarea',
  'value'  : $textarea.data('insText')
   }
 ];
 break;
  case 'size':
 var $title = 'Taille du texte';
 var $options = [
   {
  'label'  : 'Taille en pixels',
  'name'  : 'size',
  'required'   : true,
  'type'  : 'number'
   }
 ];
 break;
  case 'spoiler':
 var $title = 'Texte cach';
 var $options = [
   {
  'label'  : 'Intitul',
  'name'  : 'label',
  'type'  : 'text',
  'value'  : 'spoiler'
   },
   {
  'label'  : 'Texte',
  'name'  : 'content',
  'required'   : true,
  'type'  : 'textarea',
  'value'  : $textarea.data('insText')
   }
 ];
 break;
  case 'url':
 var $title = 'Insrer un lien';
 var $options = [
   {
  'label'  : 'Texte',
  'name'  : 'title',
  'type'  : 'text',
  'value'  : $textarea.data('insText')
   },
   {
  'label'  : 'URL',
  'name'  : 'url',
  'placeholder'  : 'http://',
  'required'   : true,
  'type'  : 'url'
   }
 ];
 break;
  case 'video':
 var $title = 'Insrer une vido';
 var $options = [
   {
  'label'  : 'Hbergeur',
  'name'  : 'host',
  'type'  : 'select',
  'options' : [
 {
   'value' : 'youtube',
   'label' : 'Youtube'
 },
 {
   'value' : 'dailymotion',
   'label' : 'Dailymotion'
 }
  ]
   },
   {
  'label'  : 'URL',
  'name'  : 'id',
 'placeholder'  : 'Ex: https://youtu.be/YPaZAVEaTYM',
  'required'   : true,
  'type'  : 'text',
  'value'  : $textarea.data('insText')
   }
 ];
   }
   var $content = '';
   for(var $i = 0; $i < $options.length; $i ++)
   {
  $content += 
  '<div class="control-group' + ($options[$i].required ? ' required' : '' ) + '">' + 
 '<label class="control-label" for="' + $options[$i].name + '">' + $options[$i].label + '</label>' + 
 '<div class="controls">';
   if($options[$i].type == 'textarea')
   {
  $content += 
  '<textarea name="' + $options[$i].name + '" id="' + $options[$i].name + '" class="input-block-level">' + ($options[$i].value ? $options[$i].value : '' ) + '</textarea>';
   }
   else if($options[$i].type == 'select')
   {
  $content += 
  '<select name="' + $options[$i].name + '" id="' + $options[$i].name + '" class="input-block-level">';
    for(var $o = 0; $o < $options[$i].options.length; $o ++)
    {
   $content += '<option value="' + $options[$i].options[$o].value + '">' + $options[$i].options[$o].label + '</option>';
    }
  $content += 
  '</select>';
   }
   else
   {
  $content += 
  '<input type="' + $options[$i].type + '" name="' + $options[$i].name + '" id="' + $options[$i].name + '" class="input-block-level" value="' + ($options[$i].value ? $options[$i].value : '' ) + '"' + ($options[$i].placeholder ? ' placeholder="' + $options[$i].placeholder + '"' : '') + '>';
   }
   $content += 
   '<input type="hidden" name="tag" value="' + $tag + '">'
 '</div>' + 
  '</div>';
   }
   $bbCodeModal.find('h3').html($title);
   $bbCodeModal.find('.modal-body').html($content);
   $bbCodeModal.modal();
 }
 else
 {
   e.preventDefault();
 }
  }
  });
 });
 $textFormat.find('[data-tag]').each(function(){
  $(this)
  .on({
  'click' : function(e){
 if(!$textarea.data('isPreview'))
 {
  layout.advancedTextarea.addTag($(this).data('tag'));
 }
 else
 {
   e.preventDefault();
 }
  }
  });
 });
 $textFormat.find('a[data-toggle="smileys"]').on('click', function(e){
   if(!$textarea.data('isPreview'))
  {
 $(this).toggleClass('active');
 $(this).parents('form').find('.smiley-container').toggleClass('hide');
  }
   else
   {
  e.preventDefault();
   }
 });
 var $preview = $('<div class="bbcode-preview hide">').css({
   'border': $textarea.css('border')
 })
 .insertBefore($textarea);
 $textFormat.find('a[data-toggle="preview"]').on('click', function(){
   if(!$preview.hasClass('open'))
   {
  $.ajax({
 url: '/themes/do/preview/',
 type: 'post',
 data: {'message' : $textarea.val()},
 dataType: 'json'
  }).done(function(json){
 $preview.css({
   'top': $textarea.position().top + 'px',
   'left': $textarea.position().left + 'px',
   'width': $textarea.outerWidth() + 'px',
   'height': $textarea.outerHeight() + 'px',
   'padding': $textarea.css('padding'),
   'border-radius': $textarea.css('border-radius')
 })
 .html(json.content);
 $preview.find('.spoiler-btn').addClass('btn-mini').removeClass('btn-link').on('click', function(){
   layout.advancedTextarea.toggleSpoiler($(this));
 });
  });
  $(this).addClass('active btn-primary').find('i').removeClass('icon-ok').addClass('icon-remove');
   }
   else
   {
  $preview.html('');
  $textarea.focus();
  $(this).removeClass('active btn-primary').find('i').removeClass('icon-remove').addClass('icon-ok');
   }
   $preview.toggleClass('open');
   $textFormat.find('.btn').not(this).toggleClass('disabled');
   $textarea.data('isPreview', $preview.hasClass('open'));
 });
 });
  $('.spoiler-btn').addClass('btn-mini').removeClass('btn-link').on('click', function(){
 layout.advancedTextarea.toggleSpoiler($(this));
  });
}
layout.advancedTextarea.addTag = function($bbOpen, $bbClose)
{
  var $textarea  = $('#' + $('#bbCodeModal').find('[name="textarea_id"]').val());
  if($('#bbCodeModal').find('.required').length)
  {
 $('#bbCodeModal').find('.required').each(function(){
   var $requiredField = $('#' + $(this).find('label').attr('for'));
   if(!$requiredField.val())
   {
  $requiredField.closest('.required:not(.error)').addClass('error');
   }
   else
   {
  $requiredField.closest('.required.error').removeClass('error');
   }
   });
  }
  if(!$('#bbCodeModal').find('.required.error').length)
  {
 if($bbOpen == undefined)
 {
   var $tag = $('#bbCodeModal').find('[name="tag"]').val();
   switch ($tag){
  case 'img':
 $bbOpen = '[img=' + $('#bbCodeModal').find('#url').val() + ' alt=' + ($('#bbCodeModal').find('#title').val() ? $('#bbCodeModal').find('#title').val() : $('#bbCodeModal').find('#url').val()) + ($('#bbCodeModal').find('#title').val() ? ' title=' + $('#bbCodeModal').find('#title').val() : '') + ($('#bbCodeModal').find('#width').val() ? ' width=' + $('#bbCodeModal').find('#width').val() : '') + ']';
 $bbClose = '';
 $textarea.data('insText', '');
 break;
  case 'quote':
 $bbOpen = '[quote' + ($('#bbCodeModal').find('#author').val() ? '=' + $('#bbCodeModal').find('#author').val() : '') + ']';
 $bbClose = '[/quote]';
 $textarea.data('insText', $('#bbCodeModal').find('#cite').val());
 break;
  case 'size':
 $bbOpen = '[size=' + $('#bbCodeModal').find('#size').val() + ']';
 $bbClose = '[/size]';
 break;
  case 'spoiler':
 $bbOpen = '[spoiler=' + ($('#bbCodeModal').find('#label').val() ? $('#bbCodeModal').find('#label').val() : 'spoiler') + ']';
 $bbClose = '[/spoiler]';
 $textarea.data('insText', $('#bbCodeModal').find('#content').val());
 break;
  case 'url':
 $bbOpen = '[url=' + $('#bbCodeModal').find('#url').val() + ']';
 $bbClose = '[/url]';
 $textarea.data('insText', $('#bbCodeModal').find('#title').val() ? $('#bbCodeModal').find('#title').val() : $('#bbCodeModal').find('#url').val());
 break;
  case 'video':
 $bbOpen = '[' + $('#bbCodeModal').find('#host').val() + ']';
 $bbClose = '[/' + $('#bbCodeModal').find('#host').val() + ']';
 $textarea.data('insText', $('#bbCodeModal').find('#id').val());
   }
 }
 $bbClose = $bbClose === undefined ? $bbOpen.replace('[', '[\/') : $bbClose;
 var $start = $textarea.data('start');
 var $end = $textarea.data('end');
   var $insText = $textarea.data('insText');
   if($insText)
   {
   var $pos = $start + $bbOpen.length + $insText.length + $bbClose.length;
   }
   else
   {
   var $pos = $start + $bbOpen.length;
   }
 $textarea.trigger('focus');
 $textarea.val($textarea.val().substr(0, $start) + $bbOpen + $insText + $bbClose + $textarea.val().substr($end));
   $textarea.prop('selectionStart', $pos);
   $textarea.prop('selectionEnd', $pos);
   $textarea.trigger('keyup');
   $('#bbCodeModal').modal('hide');
  }
  else
  {
 return false;
  }
}
layout.advancedTextarea.toggleSpoiler = function($spoilerBtn)
{
  $spoilerBtn.next('.spoiler-content').toggleClass('hide');
}
layout.resizeSlider = function(sliderId, sliderHeight)
{  
  var slider  = $('#' + sliderId);
  var slider_width  = slider.width();
  var slider_height  = sliderHeight;
  if(slider_width <= 480)
  {
 slider_height = slider_height / 2;
 slider.addClass('widget-slider-small');
  }
  else
  {
 slider.removeClass('widget-slider-small');
  }
  slider.find('.carousel-inner').css('height', slider_height + 'px');
  slider.find('.item img').each(function(){
 if($(this).get(0).height > slider_height)
 {
   var img_ratio = $(this).get(0).height / slider_height;
   var img_width = $(this).get(0).width / img_ratio;
 }
 else
 {
   var img_ratio = slider_height / $(this).get(0).height;
   var img_width = $(this).get(0).width * img_ratio;
 }
 if(img_width > slider_width)
 {
   var margin_left = (img_width - slider_width) / 2;
   $(this).css('margin-left', '-' + margin_left + 'px');
 }
 else
 {
   $(this).css('margin-left', '');
 }
  });
}
layout.formHorWidth = function()
{
  $('.view form[class*="horizontal"]').each(function(){
 if($(this).innerWidth() < 530)
 {
   $(this).addClass('form-not-horizontal').removeClass('form-horizontal');
 }
 else
 {
   $(this).removeClass('form-not-horizontal').addClass('form-horizontal');
 }
  });
}
layout.picturesMargins = function()
{ 
  $('.pictures li').each(function(){
 if($(this).parent().find('li').length > 1)
 {
   var margin_left = $(this).parent().find('li:nth-child(2)').css('margin-left');
   $(this).css('margin-bottom', margin_left);
 }
  });
}
$(document).ready(function(){
  var blocks = {};
 blocks.change_page = function(page, $blocks_rows) {
 var current_page = Number(blocks_rows.attr('data-current-page'));
 var total_page = Number(blocks_rows.attr('data-total-pages'));
 var $pagination_ul = blocks_rows.nextAll('.pagination:first');
 blocks_rows.find('.row-container').addClass('hide');
 $pagination_ul.find('.disabled').removeClass('disabled');
 if (page == 'first')
  current_page = 1;
 else if(page == 'prev')
  current_page = current_page == 1 ? current_page : current_page - 1;
 else if(page == 'next')
  current_page = current_page == total_page ? current_page : current_page + 1;
 else if(page == 'last')
  current_page = total_page;
 else 
   current_page = page;
 if (current_page == 1)
  $pagination_ul.find('.pagination-prev, .pagination-first').addClass('disabled');
 if (current_page == total_page)
  $pagination_ul.find('.pagination-next, .pagination-last').addClass('disabled');
 blocks_rows.find('.page_' + current_page).removeClass('hide');
 blocks_rows.find('.page_' + current_page+ ' .row-container').removeClass('hide');
 blocks_rows.find('.page_' + current_page + ' .cls-mobile-flash').css('width', '100%');
 blocks_rows.find('.page_' + current_page + ' .cls-mobile-flash object').css('width', '100%');
 $pagination_ul.find('.pagination-current span').text(current_page);
 $pagination_ul.find('.pagination-prev a').attr('href', '#page' + (current_page));
 $pagination_ul.find('.pagination-next a').attr('href', '#page' + (current_page));
 $('html, body').animate({
  scrollTop: blocks_rows.parents('.view:first').offset().top
 }, 'slow');
 blocks_rows.attr('data-current-page', current_page);
 location.hash = 'page' + current_page;
 return false;
 };
 var blocks_rows = $('.rows[data-total-pages]');
 var $this, $pagination_ul;
 blocks_rows.each(function() {
 var $this = $(this);
 if ($(this).attr('data-total-pages') > 1)
 {
  $pagination_ul = $this.nextAll('.pagination:first');
  $pagination_ul.find('.pagination-first a').click($.proxy(blocks.change_page, blocks, 'first', $this));
  $pagination_ul.find('.pagination-prev a').click($.proxy(blocks.change_page, blocks, 'prev', $this));
  $pagination_ul.find('.pagination-next a').click($.proxy(blocks.change_page, blocks, 'next', $this));
  $pagination_ul.find('.pagination-last a').click($.proxy(blocks.change_page, blocks, 'last', $this));
   var hash = window.location.hash;
   if (hash.substring(1, 5) == 'page')
   {  
  current_page = parseInt(hash.substring(5, hash.length), 10);
  blocks.change_page(current_page, $this);
   }
 }
 $(this).find('.row-panel').each(function(){
   var $row = $(this);
   var $collapse  = $row.find('.collapse');
   $row.addClass(function(){
  return $collapse.hasClass('in') ? 'open' : '';
   });
   $collapse.on({
  'show': function(){
 $this.find('.row-panel.open').find('.row-content').find('> .collapse.in').collapse('hide');
  },
  'shown': function(){
 console.log('SHOWN');
 $row.addClass('open');
  },
  'hidden': function(){
 console.log('HIDDEN');
 $row.removeClass('open');
  }
   });
 });
 });
});
var paymeans = {};
paymeans.pbx = {};
paymeans.pbx.cmd1  = null;
paymeans.pbx.cmd3  = null;
paymeans.pbx.total1 = null;
paymeans.pbx.total3 = null;
paymeans.pbx.hmac1 = null;
paymeans.pbx.hmac3 = null;
paymeans.pbx.datevalmax1 = null;
paymeans.pbx.datevalmax3 = null;
paymeans.pbx.selectCard = function(type)
{
  $('#PBX_TYPECARTE').val(type);
  if($('input[name=pbx_3fois]:checked').val()=='3fois')
  {
 $('#PBX_CMD').val(this.cmd3);
 $('#PBX_TOTAL').val(this.total3);
 $('#PBX_HMAC').val(this.hmac3);
 $('#PBX_DATEVALMAX').val(this.datevalmax3);
  }
  else
  {
 $('#PBX_CMD').val(this.cmd1);
 $('#PBX_TOTAL').val(this.total1);
 $('#PBX_HMAC').val(this.hmac1);
 $('#PBX_DATEVALMAX').val(this.datevalmax1);
  }
  $('input[name=pbx_3fois]').remove();
  $('#PBX_FORM').submit();
};
