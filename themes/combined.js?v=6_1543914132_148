   layout.resizeSlider($(this).attr('id'), $(this).data('height'));
 });
  }
});$(window).load(function(){
  if($('.navbar').length > 0)
  {
 var navbar_position = $('.navbar').position();
 $('.navbar-affix-clone').css({
   'display': 'block',
   'top':   navbar_position.top
 });
 $('.navbar').affix({ offset: { top: navbar_position.top } });
  }
  });